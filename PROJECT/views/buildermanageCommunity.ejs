
  <!DOCTYPE html>
  <html lang="en" dir="ltr">
    <head>
      <title>CQ</title>
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <meta charset="utf-8">
      <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
      <link rel="stylesheet" href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css">
      <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.css">
      <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.js"></script>
      <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
      <script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
      <link rel="stylesheet" href="/header.css">
      <link rel="stylesheet" href="/sidebar.css">
  
  <link rel="stylesheet" href="/managecommunitycss.css" />
  </head>
  <body>
      <% include partials/cHeader.ejs %>
      <p id="id" style="display:none;"><%= commobj._id %></p>
      <div id="viewscreen">
        <% include partials/cSidebar.ejs %>
        <div id="rightview">
          <div class="container-fluid div-min-width" style="padding:0; min-width:360px;">
            <div class="row">
              <div class="col-lg-12">
                  <% include partials/commoncommunitytop.ejs %>

                  <div class="container">
                    <div class="col-sm-12 col-xs-12 col-lg-12 col-md-12" style="padding-bottom:5px;border-bottom:1px solid #c4c4c4">
                        <nonlink class="manageCommunity-head-btn" style="cursor: context-menu;">
                          Manage Community
                        </nonlink>
                        <a href="#" style="float:right;font-weight:600">
                          Invite Users
                        </a>

                        <a class="pc" href="#" style="float:right;font-weight:600;margin-right:15px">
                          Edit Community
                        </a>
                    </div>

                    <div class="col-sm-12 col-xs-12 col-lg-12 col-md-12" style="padding:10px 0 0 0;">

                      <div class="col-sm-3 manageCommunity-commuity-div" style="padding:0">

                        <div class="panel-heading allSidesSoft center-all mobile" style="background:#fff;margin-bottom:5px">
                            <a class="btn btn-primary editcommunity-btn">
                              Edit Commuity
                            </a>
                        </div>

                        <div class="panel-body allSidesSoft" style="background:#fff;">
                            <a class="manageCommunity-child-btn manageCommunity-btn-active" onclick="showUsers()" id="UsersShowBtn">
                              Users (<%=commobj.joinedMembers.length%>)
                            </a>
                            <br>
                            <a class="manageCommunity-child-btn" onclick="showManager()" id="AdminsShowBtn">
                                Managers (0)
                            </a>

                            <br>
                            <a class="manageCommunity-child-btn" onclick="showRequest()" id="requestedUserShowBtn">
                              Requests (<%=commobj.requestedMembers.length%>)
                            </a>

                            <br>
                            <a class="manageCommunity-child-btn" onclick="showInvitation()" id="invitedUserShowBtn">
                              Invited Members (<%=commobj.invitations.length%>)
                            </a>
                        </div>
                        <br>
                      </div>
                      <div class="col-sm-9">
                        <div id="userlist">
                        </div>
                        <div id="managerlist">
                        </div>
                        <div id="requestlist">
                        </div>
                        <div id="invitationlist">
                        </div>
                      </div>

                  </div>

              </div>
            </div>
          </div>
        </div>
      </div>

  </body>
  <script src="cHeaderjs.js" type="text/javascript"></script>
<script src="cSidebarjs.js" type="text/javascript"></script> 
  <script type="text/javascript" src="/managecommunityjs.js"></script>
  </html>
